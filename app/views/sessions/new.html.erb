<h1>登入</h1>
<%= form_for @user do |f| %>
  <div class="form-group">
    <%= f.label :username, "使用者名稱" %>
    <%= f.text_field :username, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.label :password, "密碼" %>
    <%= f.password_field :password, class: "form-control" %>
  </div>
  <%= f.submit "登入", class: "btn btn-primary" %>
<% end %>
